<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qiup.programmeenquiry.qiupprogrammesenquiryapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.qiup.programmeenquiry.qiupprogrammesenquiryapplication.com.qiup.programmeenquiry.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.qiup.programmeenquiry.qiupprogrammesenquiryapplication.com.qiup.programmeenquiry.FilterProgrammes"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="Programmes Enquiry" />

        <activity
            android:name="com.qiup.programmeenquiry.qiupprogrammesenquiryapplication.com.qiup.programmeenquiry.CaptureData"
            android:label="Key-in Particulars" />

        <activity
            android:name="com.qiup.programmeenquiry.qiupprogrammesenquiryapplication.com.qiup.programmeenquiry.ResultsOfFiltering"
            android:label="Eligibility Result" />

        <activity
            android:name="com.qiup.programmeenquiry.qiupprogrammesenquiryapplication.com.qiup.programmeenquiry.InterestedProgrammeAndEnglishProficiency"
            android:label="Programmes Enquiry" />

        <activity
            android:name="com.qiup.programmeenquiry.qiupprogrammesenquiryapplication.com.qiup.programmeenquiry.InterestProgramme"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="Programmes Enquiry">
        </activity>
    </application>

</manifest>